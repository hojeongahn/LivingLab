{
  "name": "typoz",
  "version": "0.1.0",
  "description": "It was created to make it easier for users to implement their own typing effects. We are envisioning unique and diverse effects.",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch-path=\"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"ts-node src/index.ts\"",
    "build2": "rm -rf dist/ && gulp dev",
    "build": "pnpm webpack && tsc && tsc-alias",
    "webpack": "rm -rf umd/typoz.min.js && webpack",
    "test": "jest --coverage --watch __tests__/",
    "preview": "cd test && pnpm dev"
  },
  "author": {
    "name": "devkimson",
    "email": "chaplet01@gmail.com",
    "url": "https://kkn1125.github.io"
  },
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "default": "./dist/src/index.js"
      },
      "types": {
        "default": "./dist/src/index.d.ts"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnyRequest/typoz.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://anyrequest.github.io/typoz/",
  "types": "./dist/src/index.d.ts",
  "keywords": [
    "react",
    "javascript",
    "library",
    "typescript",
    "typing",
    "write",
    "typing-effect",
    "typing-builder",
    "korean-typing-effect",
    "korean-typing"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/register": "^7.22.15",
    "@types/gulp": "^4.0.17",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "gulp": "^4.0.2",
    "gulp-minify": "^3.1.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "path": "^0.12.7",
    "reflect-metadata": "^0.2.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^3.15.0",
    "tsify": "^5.0.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "pnpm": {
    "overrides": {
      "terser@<4.8.1": ">=4.8.1",
      "glob-parent@<5.1.2": ">=5.1.2",
      "es5-ext@>=0.10.0 <0.10.63": ">=0.10.63"
    }
  }
}
